ods noproctitle;
ods graphics / imagemap=on;

proc corr data=WORK.IMPORT1 pearson nosimple rank plots=matrix(histogram);
	var Log_ICC_Minutes Log_IHBS_Minutes Log_TBS_Minutes;
	with Log_Crisis_Int_Minutes;
run;

ods noproctitle;
ods graphics / imagemap=on;

proc reg data=WORK.IMPORT1 alpha=0.05 plots(only)=(diagnostics residuals 
		observedbypredicted);
	model Log_Reunified=Log_ICC_Minutes Log_IHBS_Minutes Log_TBS_Minutes /;
	run;
quit;
